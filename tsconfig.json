{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./docs/",
    "noImplicitAny": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "importHelpers": true,
    "importsNotUsedAsValues": "preserve",
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM",
      "ScriptHost"
    ]
  }
}
